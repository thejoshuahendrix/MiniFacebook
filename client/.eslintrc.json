{
    "extends": [
        "plugin:cypress/recommended",
        "plugin:lvksh/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "ignorePatterns": [
        "!**/*"
    ],
    "plugins": [
        "lvksh"
    ],
    "env": {
        "node": true
    },
    "rules": {
        "unicorn/consistent-function-scoping":"off"
    }
}